packages: ./ghcup.cabal

optional-packages: ./3rdparty/*/*.cabal

optimization: 2

package streamly
    ghc-options: -O2 -fspec-constr-recursive=16 -fmax-worker-args=16

package ghcup
    ghc-options: -O2 -fspec-constr-recursive=16 -fmax-worker-args=16

package tar-bytestring
    ghc-options: -O2

constraints: http-io-streams -brotli

allow-newer: base
